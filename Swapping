import java.util.*;
public class Swapping{
    public static void swap(int a, int b){
        a = a+b; 
        b = a-b ;
        a = a-b;
        System.out.println("a " + a);
        System.out.println("b " + b);
    }     
    public static void main(String args[]){
      int a = 5; 
      int b = 9;
      swap(a, b);
    }
}
